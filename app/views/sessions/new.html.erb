<h4> Enter your details to log in</h4>
	<div class="well">
	  <%= form_tag login_path do %>
		<%= label_tag :username %>
		<%= text_field_tag :username %>
		<br />
		<%= label_tag :password %>
		<%= password_field_tag :password %>
		<br />
		<%= submit_tag "Log In", class: 'btn btn-primary'%>
		<% end %>
	</div> 